    console.log(incoming);
}

//Solution 12 - Woah, That sucker's Huge!

var sum = 0;
for(var i = -300000; i <= 300000; i++){
    if(i%2 !== 0){
        sum += i;
    }
    console.log(sum);
}

//Solution 13 - Countdown by Fours

var num = 2016
while(num>0){
    console.log(num);
    num -=4;
}

// Solution 14 - Flexible Countdown

function countdown(lowNum,highNum,mult){
for(var i = highNum; i > lowNum; i=1){
        if(i%mult === 0){
            console.log(i);
        }
    }
}
countdown(2,9,3);

// Solution 15 - Final Countdown

function countdown(param1,param2,param3,param4){
    var num = param2;
    while(num<=param3){
        if(num%param1===0){
            if(num !== param4){
                console.log(num);
            }
        }
        num++;
    }    
}
countdown(3,5,17,19);
